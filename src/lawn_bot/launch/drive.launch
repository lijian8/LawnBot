<launch>
    <!-- map to odom transform -->
    <node pkg="tf" type="static_transform_publisher" name="map_to_odom" args="0 0 0 0 0 0 map odom 30" />

    <!-- sends left and right wheel speeds  -->
    <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
        <param name="port" value="/dev/ttyACM0"/>
    </node>

    <!-- odom to base_link transform -->
    <node pkg="lawn_bot" type="differential_tf.py" name="differential_tf"> 
    </node>

    <include file="$(find lawn_bot)/launch/map_server.launch" />

    <!-- Move Base Node -->
  <!-- Subscribe: /move_base_simple/goal -->
  <!-- Publish: /cmd_vel -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find lawn_bot)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find lawn_bot)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find lawn_bot)/params/local_costmap_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find lawn_bot)/params/global_costmap_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find lawn_bot)/params/base_local_planner_params.yaml" command="load" />
  </node>


    
</launch>
